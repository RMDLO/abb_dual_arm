<launch>

  <arg name="serial_no_camera1" value="017322075060" />
  <arg name="serial_no_camera2" value="130322270317" />
  <arg name="camera1" default="d435"/>
  <arg name="camera2"	default="d405"/>
  <arg name="camera3"	default="oak"/>
